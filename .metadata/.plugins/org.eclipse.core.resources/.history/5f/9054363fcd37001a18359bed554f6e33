
public class TaskVar {

	/**
	 * @param args
	 * 125 + 24
	* 125 - 24
	* 125 x 24
	* 125 / 24
	* 125 mod 24
	 */
	public static void main(String[] args) {
		int numAdd;
		numAdd = 125 + 24;
		
		int num1;
		numAdd1 = 125 - 24;
		
		System.out.println(numAdd + " = 125 +24");
		System.out.println(numAdd1 + "125 - 24");
	}

}
