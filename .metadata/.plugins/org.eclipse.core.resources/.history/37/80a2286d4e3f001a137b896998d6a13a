package lv.renata.player;

public class FootballPlayer {
	String name;
	String position;
	int ShirtNumber;
	boolean build;
	long height;
	int weight;
	
	
	public String getName(){
		return name;
	}
	public String getPosition(){
		return position;
	}
	public int getShirtNumber(){
		return ShirtNumber;
	}
	public boolean getBuild(){
		return build;
	}
	public long getheight(){
		return height;
		
	}
	public int getWeight(){
		return weight;
	}
}
