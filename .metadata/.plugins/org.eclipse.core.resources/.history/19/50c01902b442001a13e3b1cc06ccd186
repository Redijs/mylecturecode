package lv.renata;

import lv.renata.figure.Rectangle;

public class TestFigure {

	
	public static void main(String[] args) {
		Rectangle rectangle = new Rectangle();
		System.out.println(rectangle.toString()); //atgrieza veertiibas, kas failaa Rectangle nodefineetas ar 0

		Rectangle rectangle2 =new Rectangle(0.0f, 0.0f); //otro konstruktoru testeejam
		System.out.println(rectangle2);
		
		Rectangle rectangle3 = new Rectangle (2.5f);
		System.out.println(rectangle3);
	}

}
