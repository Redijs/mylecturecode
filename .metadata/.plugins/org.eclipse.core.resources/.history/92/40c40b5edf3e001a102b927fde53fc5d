package lv.renata.learnjavaclass;

public class Box {
	double width; //shei parametri definee Boxu, bez shiem parametriem box nebuus iespeejams. 
	double depth; //Bet mees nezinam sho parametru veertiibas.
	double height;
	
	public double getVolume (){
		return width * height  * depth;
		
	}
	volume = boxObject.width * boxObject.height * boxObject.depth;
	
}
