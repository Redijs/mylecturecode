package lv.renata;

import java.util.ArrayList;

public class TestArrayListCars {

	
	public static void main(String[] args) {
		ArrayList <String> cars = new ArrayList<>(); 
		
		cars.add("Volvo"); 
		cars.add("BMW");
		cars.add("Ford");
		cars.add("Ford");
		
		System.out.println(cars);
		
		for  (String c : cars) {
			 System.out.println(c);
			}

		
	}

}
