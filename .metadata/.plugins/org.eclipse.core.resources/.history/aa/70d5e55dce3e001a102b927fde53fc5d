package lv.renata;

public class LearnMethodsWithType {

	/**
	method return type double
	a*b= result
	 */
	public static void main(String[] args) {
		getArea ();
		System.out.println(getArea());

	}
	public static double getArea(){
		double num1 = 20.3;
		double num2 = 10.0;
		
		double result = num1*num2;
		return result;

	}
}
