
public class PrimType {

	
	public static void main(String[] args) {
		//numeric data primitive type
		int integer = 2;
		long longInteger = 20204040240L;
		short shortInteger = 2020;
		byte byteInteger = 120;
		//Floating-Point type real numbers
		double doubleFloatNumber = 0.5d;
		float floatNumber = 0.12412412f;
		//Characters
		char charSymbol = 'a'; 
		char charSymbolAsNumber = 33; 
		System.out.println(" Char:" +charSymbolAsNumber);
		char charSymbolAsNumber1 = 82;//savs vards
		char charSymbolAsNumber2 = 69;
		char charSymbolAsNumber3 = 78;
		char charSymbolAsNumber4 = 65;
		char charSymbolAsNumber5 = 84;
		
		
		System.out.println("" + charSymbolAsNumber1 + charSymbolAsNumber2 + charSymbolAsNumber3 + charSymbolAsNumber4 + charSymbolAsNumber5 + charSymbolAsNumber2);
		

	}

}
